    },
    {
      "id": 59,
      "text": "Along with the ab erration and rotation, more advanced cosmological effects may be detected using",
      "label_id": 5,
      "label_name": "First-Line",
      "is_meta": false,
      "parent": 53,
      "rel_id": 2,
      "rel_name": "equality",
